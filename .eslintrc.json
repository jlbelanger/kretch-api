{
	"extends": "airbnb-base",
	"env": {
		"mocha": true
	},
	"rules": {
		"arrow-parens": ["error", "always"],
		"global-require": 0,
		"indent": ["error", "tab", { "SwitchCase": 1 }],
		"max-len": ["error", { "code": 150 }],
		"no-continue": 0,
		"no-tabs": 0,
		"no-param-reassign": 0,
		"no-use-before-define": 0,
		"prefer-destructuring": 0
	}
}
